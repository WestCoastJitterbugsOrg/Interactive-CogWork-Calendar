{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cw-addons/cw-filter-calendar",
	"icon": "calendar-alt",
	"title": "Cw Filter Calendar",
	"category": "widgets",
	"description": "Calendar using data from cogwork that allows filtering, reading event information and registration",
	"supports": {
		"html": false
	},
	"textdomain": "cw-filter-calendar",
	"editorScript": "file:src/index.tsx",
	"editorStyle": "file:src/index.css",
	"viewScript": "file:src/view.tsx",
	"style": "file:src/view.css",
	"attributes": {
		"Organization": {
			"type": "string",
			"default": ""
		},
		"Password": {
			"type": "string",
			"default": ""
		},
		"Colors": {
			"type": "object",
			"default": {
				"primary": "#ab2814",
				"primary-alt": "#ec6350",
				"secondary": "#349995",
				"secondary-alt": "#73bdba",
				"dark": "#1d1d1b",
				"light": "#fffaf2"
			}
		}
	}
}
