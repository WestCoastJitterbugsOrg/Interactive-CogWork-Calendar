{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"outDir": "dist",
		"allowJs": true,
		"skipLibCheck": true,
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"sourceMap": true,
		"baseUrl": ".",
		"lib": ["ESNext", "dom"],
		"allowSyntheticDefaultImports": true
	},
	"include": ["src/**/*"]
}
