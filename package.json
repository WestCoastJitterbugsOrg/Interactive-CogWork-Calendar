{
  "name": "personalized-wcj-calendar",
  "version": "1.0.0",
  "description": "Get an overview of your schedule",
  "private": "true",
  "scripts": {
    "test": "tsc --outDir dist/test && nyc ava",
    "build": "webpack --config webpack.dev.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "start:prod": "webpack --config webpack.prod.js && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WestCoastJitterbugsOrg/Personalized-Calendar.git"
  },
  "author": "Jean-Philippe Green",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WestCoastJitterbugsOrg/Personalized-Calendar/issues"
  },
  "homepage": "https://the-wcj.guru/personal-calendar/",
  "dependencies": {
    "@fullcalendar/core": "^5.4.0",
    "@fullcalendar/daygrid": "^5.4.0",
    "@fullcalendar/list": "^5.4.0",
    "@fullcalendar/timegrid": "^5.4.0",
    "color-hash": "^1.0.3",
    "dayjs": "^1.9.6",
    "express": "^4.17.1",
    "ics": "^2.26.1",
    "jquery": "^3.5.1",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/color-hash": "^1.0.0",
    "@types/express": "^4.17.9",
    "@types/gapi": "0.0.39",
    "@types/gapi.calendar": "^3.0.4",
    "@types/jquery": "^3.5.4",
    "@types/md5": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "autoprefixer": "^10.0.2",
    "ava": "^3.13.0",
    "ava-browser-fixture": "^2.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.22.1",
    "html-webpack-plugin": "^4.5.0",
    "nyc": "^15.1.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "ava": {
    "verbose": true,
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "tsconfig-paths/register"
    ],
    "files": [
      "test/**/*.ts"
    ],
    "extensions": [
      "ts"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "src/index.ts"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}
