{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/list": "^5.9.0",
    "@fullcalendar/react": "^5.9.0",
    "@fullcalendar/timegrid": "^5.9.0",
    "@popperjs/core": "^2.11.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-modal": "^3.14.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@types/jest": "^27.4.1",
    "@types/jquery": "^3.5.14",
    "@types/node": "^17.0.23",
    "@types/postcss-import": "^14.0.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-modal": "^3.13.1",
    "@types/tailwindcss": "^3.0.0",
    "@types/webpack": "^5.28.0",
    "@types/zip-webpack-plugin": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "path": "^0.12.7",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-rem-to-pixel": "^4.1.2",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-sources": "^3.2.3",
    "zip-webpack-plugin": "^4.0.1"
  },
  "scripts": {
    "start:standalone": "yarn webpack serve --config webpack.standalone-config.js",
    "start:wordpress": "yarn webpack watch --config webpack.wordpress-config.js",
    "build:prod": "yarn webpack --config webpack.wordpress-config.js --env production",
    "test": "jest"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react/jsx-runtime",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-definitions": [
        "error",
        "interface"
      ]
    },
    "ignorePatterns": [
      "public/**/*",
      "build/**/*",
      "*config.js",
      "*.d.ts"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.1.1"
}
