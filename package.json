{
  "name": "personalized-wcj-calendar",
  "version": "1.0.0",
  "description": "Get an overview of your schedule",
  "private": "true",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "start:prod": "webpack --config webpack.prod.js && node server.js",
    "test": "jest --config jest.config.js --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WestCoastJitterbugsOrg/Personalized-Calendar.git"
  },
  "author": "Jean-Philippe Green",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WestCoastJitterbugsOrg/Personalized-Calendar/issues"
  },
  "homepage": "https://the-wcj.guru/personal-calendar/",
  "dependencies": {
    "@fullcalendar/core": "^5.7.0",
    "@fullcalendar/daygrid": "^5.7.0",
    "@fullcalendar/list": "^5.7.0",
    "@fullcalendar/timegrid": "^5.7.0",
    "color-hash": "^1.1.1",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "ics": "^2.27.0",
    "jquery": "^3.6.0",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/color-hash": "^1.0.0",
    "@types/express": "^4.17.11",
    "@types/gapi": "0.0.39",
    "@types/gapi.calendar": "^3.0.4",
    "@types/jest": "^26.0.23",
    "@types/jquery": "^3.5.5",
    "@types/md5": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "autoprefixer": "^10.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.23.0",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^26.6.3",
    "nyc": "^15.1.0",
    "sass": "^1.32.13",
    "sass-loader": "^10.2.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.2.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "src/index.ts",
      "src/app/default-objects.ts",
      "**/*.d.ts",
      "**/*types.ts"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}
