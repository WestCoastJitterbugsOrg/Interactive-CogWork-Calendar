{
  "name": "wcj-calendar",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@fullcalendar/daygrid": "^5.11.0",
    "@fullcalendar/interaction": "^5.11.3",
    "@fullcalendar/list": "^5.11.3",
    "@fullcalendar/react": "^5.11.1",
    "@fullcalendar/timegrid": "^5.11.3",
    "@popperjs/core": "^2.11.5",
    "ics-js": "^0.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@prettier/plugin-php": "^0.18.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^28.1.7",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-modal": "^3.13.1",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.14",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "^13.0.3",
    "path": "^0.12.7",
    "postcss": "^8.4.20",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.1.7",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-sources": "^3.2.3"
  },
  "scripts": {
    "start": "yarn start:wordpress",
    "start:standalone": "yarn webpack serve --config webpack.standalone-config.js",
    "start:wordpress": "yarn webpack watch --config webpack.wordpress-config.js",
    "build": "yarn build:prod",
    "build:prod": "yarn webpack --config webpack.wordpress-config.js --env production",
    "test": "jest --coverage",
    "test-ci": "jest --ci --coverage",
    "postinstall": "husky install",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react/jsx-runtime",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "prettier"
    ],
    "ignorePatterns": [
      "public/**/*",
      "build/**/*",
      "dist/**/*",
      "coverage/**/*",
      "*config.js",
      "*.d.ts"
    ],
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "overrides": [
      {
        "files": ".tsx",
        "rules": {
          "@typescript-eslint/consistent-type-definitions": [
            "error",
            "interface"
          ],
          "react/button-has-type": [
            "warn"
          ]
        }
      }
    ]
  },
  "lint-staged": {
    "{src,test,public}/**/*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.1.1"
}
